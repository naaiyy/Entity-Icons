{
  "name": "@entityy/entity-icons",
  "version": "0.0.14",
  "description": "Modern React icon library with ESM support and full TypeScript types",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./icons/*": "./dist/icons/*.js"
  },
  "files": [
    "dist",
    "apple",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsx scripts/build-icons.ts && tsup",
    "build:apple": "tsx scripts/build-apple-icons-png.ts",
    "build:all": "pnpm run build && pnpm run build:apple",
    "dev": "tsup --watch",
    "typecheck": "tsc --noEmit",
    "lint": "biome check .",
    "lint:fix": "biome check --write .",
    "format": "biome format --write .",
    "test": "vitest run",
    "test:watch": "vitest",
    "prepublishOnly": "pnpm run build:all"
  },
  "peerDependencies": {
    "react": ">=18.0.0"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@testing-library/react": "^16.0.1",
    "@types/node": "^22.9.0",
    "@types/react": "^18.3.12",
    "happy-dom": "^15.7.4",
    "react": "^18.3.1",
    "tsup": "^8.3.5",
    "tsx": "^4.19.2",
    "typescript": "^5.6.3",
    "vitest": "^2.1.5"
  },
  "keywords": [
    "icons",
    "react",
    "svg",
    "components",
    "entity",
    "esm",
    "typescript"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/entityy/entity-icons.git"
  },
  "author": "naaiyy1",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0"
  }
}
